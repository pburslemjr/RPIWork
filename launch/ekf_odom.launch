<launch>
  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_loc_node" clear_params="true">
    <rosparam command="load" file="$(find tf_tree_publisher)/params/ekf_odom.yaml" />
    
    <!--  Placeholder for output topic remapping
    <remap from="/odometry/filtered" to=""/>
    <remap from="accel/filtered" to=""/>
    -->

  </node>
  <node pkg="robot_localization" type="navsat_transform_node" name="navsat_trans_node" clear_params="true">
    <rosparam command="load" file="$(find tf_tree_publisher)/params/navsat_transform.yaml" />
    <remap from="imu/data" to="/data"/>
    <remap from="gps/fix" to="/fix"/>
  </node>
</launch>
